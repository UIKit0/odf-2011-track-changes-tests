namespace ac="http://www.deltaxml.com/ns/track-changes/attribute-change-namespace" 
namespace split="http://www.deltaxml.com/ns/track-changes/split-namespace"

include "common-sans.rnc"

sectioncore = 
   element text:section {
      attribute * {text}*,
      element text:p { 
         "This has a table! Yay for the moon landing.",
         ( attribute text:* { text } )*
      },
      element table:table {
         attribute table:name {"Table1"},
         attribute table:style-name {"Table1"},
         element table:table-column { anything },
         element table:table-column { anything },
         element table:table-column { anything },
         element table:table-row { anything },
         element table:table-row { anything }
      },
      element text:p { 
         "But when will we return?",
         ( attribute text:* { text } )*
      }
   }
