sectioncore = 
   element text:section {
      attribute * {text}*,
      element text:p { 
         "Here we have a lovely little Koala. He doesnâ€™t need any change tracking, lethargic as he is.",
         ( attribute text:* { text } )*
      },
      element text:p { 
         element draw:frame {
            attribute text:anchor-type {"as-char"},
            attribute draw:z-index {"0"},
            attribute draw:style-name {"graphic1"},
            attribute svg:width {text},
            attribute svg:height {text},
            element draw:image {
               attribute xlink:href {text},
               attribute xlink:type {"simple"},
               attribute xlink:show {"embed"},
               attribute xlink:actuate {"onLoad"}
            }
         },
         ( attribute text:* { text } )*
      }
   }
